<h1>Login</h1>
<section class="contCenter">  
    <form action="/api/sessions/login" method="POST" class="form">
        <content class="log">
            <div class="campo">
                <input type="email" id="email" name="email" placeholder="Email" required>
            </div>
            <div class="campo">
                <input type="password" id="password" name="password" placeholder="Password" required>
            </div>
        </content>
        <content class="log">
            <button type="submit">Login</button>
            <button class="btnSec"><a href="/register">Register</a></button>
        </content>
    </form>
    <content class="log">
        <button class="btnSec"><a href="/api/sessions/github">GitHub</a></button>
    </content>
    
</section>