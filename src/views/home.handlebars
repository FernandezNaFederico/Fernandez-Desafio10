<section class="mainCont">
    <h1>Home</h1>
    <main class="section">
        {{#each allProds}}
        <div class="card cardFront">
            <img src="https://i.pinimg.com/originals/c0/d1/84/c0d184afe02df1b20aab8ea28011507b.jpg" alt="furniture">
            <div class="text">
                <h2>{{this.title}}</h2>
                <p>Description:</p>
                <strong>{{this.description}}</strong>
                <p>Price: <strong>{{this.price}}</strong> $</p>
                <button type="button"><a href="http://localhost:8080/products/{{this._id}}">Buy Now</a></button>
            </div>
        </div>
        {{/each}}
    </main>
</section>